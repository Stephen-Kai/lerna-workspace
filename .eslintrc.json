{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-console": "warn"
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
   "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          ["@Stephen-Kai/lerna-pkg-1", "./packages/lerna-pkg-1/src"],
          ["@Stephen-Kai/lerna-pkg-2", "./packages/lerna-pkg-2/src"],
          ["@Stephen-Kai/lerna-pkg-3", "./packages/lerna-pkg-3/src"],
          ["@Stephen-Kai/lerna-pkg-4", "./packages/lerna-pkg-4/src"]
         
        ]
      }
    }
  }
}
